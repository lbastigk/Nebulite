{
  "binaries" : [
    "./bin/Nebulite --headless",
    "./bin/Nebulite_Debug --headless",
    "wine ./bin/Nebulite.exe --headless",
    "wine ./bin/Nebulite_Debug.exe --headless"
  ],
  "ignore_lines": {
    "cout" : [],
    "cerr" : [
      "0114:err:d3d:wined3d_swapchain_resize_buffers"
    ]
  },
  "tests": [
    {
      "command": "help",
      "expected": { "cout": null, "cerr": [] }
    },
    {
      "command": "echo 1234",
      "expected": { "cout": ["1234"], "cerr": [] }
    },
    {
      "command": "error 1234",
      "expected": { "cout": [], "cerr": ["1234"] }
    },
    {
      "command": "'echo testing multiline echo ; echo this is the second line'",
      "expected": { 
        "cout": [
          "testing multiline echo", 
          "this is the second line"
        ], 
        "cerr": [] 
      }
    },
    {
      "command": "'set-fps 60 ; wait 30 ; exit'",
      "expected": { "cout": [], "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/snapshot.nebs",
      "expected": { "cout": ["Loading task list from file: TaskFiles/Tests/Short/snapshot.nebs"], "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/echo_global_value.nebs",
      "expected": { 
        "cout": [
          "Loading task list from file: TaskFiles/Tests/Short/echo_global_value.nebs",
          "0.001000",
          "1.001000"
        ], 
        "cerr": [] 
      }
    },
    {
      "command": "task TaskFiles/Tests/Short/print_time.nebs",
      "expected": { 
        "cout": [
          "Loading task list from file: TaskFiles/Tests/Short/print_time.nebs", 
          "Testing global access", 
          "", 
          "Framecount is: 30"
        ], 
        "cerr": [] 
      }
    },
    {
      "command": "task TaskFiles/Tests/Short/init_print_global.nebs",
      "expected": { "cout": null, "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/push_pop.nebs",
      "expected": {
        "cout": null, 
        "cout_actually_fix_pushpop_first": [
          "Loading task list from file: TaskFiles/Tests/Short/push_pop.nebs",
          "{",
          "    \"zzy\": [",
          "        \"\",",
          "        \"0\",",
          "        \"1\",",
          "        \"2\",",
          "        \"\"",
          "    ],",
          "    \"zzz\": [",
          "        \"1\",",
          "        \"2\"",
          "    ]",
          "}"
        ], "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/json_retrieval.nebs",
      "expected": { 
        "cout": [
          "Loading task list from file: TaskFiles/Tests/Short/json_retrieval.nebs", 
          "Printing value retrieved from: ./Resources/Renderobjects/standard.jsonc:text.color.A",
          "Expected value: 255", 
          "255"
        ], 
        "cerr": [] 
      }
    },
    {
      "command": "task TaskFiles/Tests/Short/Object_spawn_and_state_printing.nebs",
      "expected": { "cout": null, "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/Object_out_of_bounds.nebs",
      "expected": { "cout": null, "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/gravity_classic_some_frames.nebs",
      "expected": { 
        "cout": null, "cerr": [] }
    },
    {
      "command": "task TaskFiles/Tests/Short/Pong_for_5s.nebs",
      "expected": { "cout": null, "cerr": [] }
    }
  ],
  "timeout": 40
}