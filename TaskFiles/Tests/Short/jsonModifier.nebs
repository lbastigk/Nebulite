# Tests json modifier usage
set-fps 60
wait 1
set test.MyKey 3
wait 1
eval echo {global.test.MyKey}                   # Should print 3
eval echo {global.test.MyKey|add 2|toInt}       # Should print 5
eval echo {global.test.MyKey|mod 2|toInt}       # Should print 1
eval echo {global.test.MyKey|multiply 2|toInt}  # Should print 6
set test.arr[0] 10
set test.arr[1] 20
set test.arr[2] 30
eval echo {global.test.arr|length|toInt}        # Should print 3
eval echo {global.test.arr|at 1|toInt}          # Should print 20
print test
exit