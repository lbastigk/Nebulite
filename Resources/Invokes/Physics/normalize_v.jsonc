{
    "topic": "",
    "condition": "$(neq({self.physics.vX},0) + neq({self.physics.vY},0))",
    "action": {
        "assign": [
            "self.physics.v = $( sqrt( {self.physics.vX}^2 + {self.physics.vY}^2 ) )",
            "self.physics.vX = $( {self.physics.vX} /  {self.physics.v} )",
            "self.physics.vY = $( {self.physics.vY} /  {self.physics.v} )"
        ],
        "functioncall": {
            "global": [],
            "self": [],
            "other": []
        }
    }
}