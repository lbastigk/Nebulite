{
  "$schema": "https://raw.githubusercontent.com/microsoft/vscode-textmate/master/grammar.schema.json",
  "name": "Nebulite Script",
  "scopeName": "source.nebs",
  "patterns": [
    {
      "name": "comment.line.number-sign.nebs",
      "match": "#.*$"
