{
  "name": "nebulite-script",
  "displayName": "Nebulite Script",
  "description": "Language support for Nebulite script files (.nebs)",
  "version": "0.0.1",
  "publisher": "lbastigk",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nebs",
        "aliases": ["Nebulite Script", "nebs"],
        "extensions": [".nebs"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nebs",
        "scopeName": "source.nebs",
        "path": "./syntaxes/nebs.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Nebulite Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Nebulite-color-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lbastigk/Nebulite.git"
  }
}