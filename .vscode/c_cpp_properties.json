{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/external/SDL2/include",
                "${workspaceFolder}/external/SDL2_image/include",
                "${workspaceFolder}/external/SDL2_ttf/",
                "${workspaceFolder}/external/rapidjson/include",
                "${workspaceFolder}/external/tinyexpr",
                "${workspaceFolder}/external/abseil"
            ],
            "defines": [
                "SDL_DISABLE_IMPLEMENTS"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-clang-x64",
            "compileCommands": "${workspaceFolder}/tmp/build_linux-debug/compile_commands.json",
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}
