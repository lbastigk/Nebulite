{
    "type": "continous",
    "logicalArg": "($(self.id) != $(other.id)) and $(other.physics.isGrav)",
    "self": {
        "physics.aX": [
            "add"
            "$(global.G) * $(other.physics.mass) * ( $(other.posX) - $(self.posX)  ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )"
        ],
        "physics.aY": [
            "add",
            "$(global.G) * $(other.physics.mass) * ( $(other.posY) - $(self.posY)  ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )"
        ]
    },
    "other": {
        "physics.aX": [
            "add"
            "$(global.G) * $(self.physics.mass)  * ( $(self.posX)  - $(other.posX) ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )"
        ],
        "physics.aY": [
            "add",
            "$(global.G) * $(self.physics.mass)  * ( $(self.posY)  - $(other.posY) ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )"
        ]
    },
    "global": {
    }
}
