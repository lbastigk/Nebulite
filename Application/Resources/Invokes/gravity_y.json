{
    "type": "continous",
    "logicalArg": "($(self.id) != $(other.id)) and $(other.physics.isGrav)",
    "selfChangeType": "add",
    "selfKey": "physics.aY",
    "selfValue": "$(global.G) * $(other.physics.mass) * ( $(other.posY) - $(self.posY)  ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )",
    "otherChangeType": "add",
    "otherKey": "physics.aY",
    "otherValue": "$(global.G) * $(self.physics.mass)  * ( $(self.posY)  - $(other.posY) ) / ( ( ($(other.posX) - $(self.posX))^2 + ($(other.posY) - $(self.posY))^2 + 1)^(3/2) )",
    "globalChangeType": "",
    "globalKey": "",
    "globalValue": ""
}
