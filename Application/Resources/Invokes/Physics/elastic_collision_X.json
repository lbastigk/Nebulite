{
  "topic" : "collision",
  "comment0" : "Collision ruleset for reflecting X velocity",
  "comment1" : "All in logicalArg array must be true",
  "logicalArg": [
    "neq($(global.time.t), $(self.collision.time.lastX))",
    "neq($(self.id),          $(other.id))",
    "$(other.physics.mass)",
    " lt($(self.posX),        $(other.posX) + $(other.sprite.sizeX))",
    " lt($(self.posY),        $(other.posY) + $(other.sprite.sizeY))",
    " lt($(other.posX),       $(self.posX)  + $(self.sprite.sizeX))",
    " lt($(other.posY),       $(self.posY)  + $(self.sprite.sizeY))",
    "not(  lt($(self.posY)+$(self.sprite.sizeY) - 2,$(other.posY)) + lt($(other.posY)+$(other.sprite.sizeY) - 2,$(self.posY))  )"
  ],
  "isGlobal": true,
  "self_invokes": [
    {
      "changeType": "set",
      "key": "physics.vX",
      "value": "$(0.99*$($(self.physics.vX) - (2 * $(other.physics.mass) / ($(self.physics.mass) + $(other.physics.mass))) * ($(self.physics.vX) - $(other.physics.vX))))"
    },
    {
      "changeType": "set",
      "key": "physics.aX",
      "value": "0"
    },
    {
      "changeType": "set",
      "key": "physics.aY",
      "value": "0"
    },
    {
      "changeType": "set",
      "key": "collision.time.lastX",
      "value": "$($(global.time.t) + 0)"
    }
  ],
  "other_invokes": [],
  "global_invokes": [],
  "functioncalls": [
    "eval echo Collision, reflecting X at $(global.time.t_ms) ms"
  ]
}