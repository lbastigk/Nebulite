{
    "type": "loop",
    "logicalArg": "($(global.mouse.delta.left) > 0) and ($(global.mouse.current.X) > $(self.posX)) and ($(global.mouse.current.Y) > $(self.posY)) and ($(global.mouse.current.X) < ($(self.posX)+$(self.pixSizeX))) and ($(global.mouse.current.Y) < ($(self.posY)+$(self.pixSizeY)))",
    "selfChangeType": "setStr",
    "selfKey": "text.str",
    "selfValue": "$(global.click.count)",
    "otherChangeType": "",
    "otherKey": "",
    "otherValue": "",
    "globalChangeType": "add",
    "globalKey": "click.count",
    "globalValue": "1"
}
